- content.each do |node|
  - if node.class == String
    %p= node.render_resource.html_safe
  - elsif node.class == Hash && node["type"] == "table"
    %table
      - node["content"].each do |row|
        %tr
          - row.each do |cell|
            %td
              - cell.each do |para|
                %p= para.render_resource.html_safe

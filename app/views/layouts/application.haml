!!!
%html
  %head
    %title MathLib
    = stylesheet_link_tag "application", :media => "all"
    = javascript_include_tag "application"
    = yield :app_head
    = csrf_meta_tags
  %body
    .jesse-notification
      - flash.each do |name, msg|
        %span= msg
    #wrap
      = yield :app_nav
      = yield :app_content
      #push
    #footer
      .container
        %p.muted.credit
          = link_to "ChangeLog", { controller: "/admin/homeworks", action: "index" }, :method => "get"
          %span= "|"
          = link_to "Feedback", { controller: "/admin/homeworks", action: "index" }, :method => "get"
